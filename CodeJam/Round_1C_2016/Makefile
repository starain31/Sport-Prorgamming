a.exe: a.cpp
	g++ --std=c++11 -o a.exe main.cpp a.cpp
a.out: a.exe a.in
	./a.exe < a.in > a.out

b.exe: b.cpp
	g++ --std=c++11 -o b.exe main.cpp b.cpp
b.out: b.exe b.in
	./b.exe < b.in > b.out

c.exe: c.cpp
	g++ --std=c++11 -o c.exe main.cpp c.cpp
c.out: c.exe c.in
	./c.exe < c.in > c.out

d.exe: d.cpp
	g++ --std=c++11 -o d.exe main.cpp d.cpp
d.out: d.exe d.in
	./d.exe < d.in > d.out
